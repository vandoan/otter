<div class="page-header"> 
	<div> 
		<h1><%= @user.full_name %></h1> 
	</div> 
	<div> 
		<h4> Favorite Books </h4> 
		  <p> <%= @user.favorite_books %> </p> 
	</div>
</div> 

<% if @statuses %> 
	<% @statuses.each do |status| %>
		<div class='well'> 
		<%= status.content %> 
		<hr /> 
		<%= link_to  time_ago_in_words(status.created_at), status_path(status) %> ago
		</div>  

		<div class='well'> 
			<hr /> 
		</div> 
	<% end %> 
<% end %> 
